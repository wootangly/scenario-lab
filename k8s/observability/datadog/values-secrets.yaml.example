# Datadog Secrets Configuration (EXAMPLE)
# Copy this file to values-secrets.yaml and add your actual secrets
# values-secrets.yaml is gitignored for security

datadog:
  # Your Datadog API Key
  # Get from: https://app.datadoghq.com/organization-settings/api-keys
  apiKey: "your-datadog-api-key-here"
  
  # Optional: App Key for advanced features
  # Get from: https://app.datadoghq.com/organization-settings/application-keys
  appKey: "your-datadog-app-key-here"

